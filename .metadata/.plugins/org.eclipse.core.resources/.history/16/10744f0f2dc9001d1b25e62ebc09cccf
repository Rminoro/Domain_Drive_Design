package br.com.fiap.exercicio;

public class ContaCorrente {
	
	double saldo;
	Cliente titular = new Cliente();
	
	void depositar(double valor) {
		saldo += valor;
	}
	
	void retirar(double valor) {
		saldo -= valor;
	}
	
	public double retornarSaldo() {
		System.out.println(saldo);	
		return saldo;	
	}
	
}